<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Demo data simplified to avoid dependencies on commented fees element lines -->
    
        <record id="op_fees_term_line_1" model="op.fees.terms.line">
            <field name="due_days">15</field>
            <field name="value">30.0</field>
        </record>

        <record id="op_fees_term_line_2" model="op.fees.terms.line">
            <field name="due_days">30</field>
            <field name="value">30.0</field>
        </record>

        <record id="op_fees_term_line_3" model="op.fees.terms.line">
            <field name="due_days">60</field>
            <field name="value">40.0</field>
        </record>

        <record id="op_fees_term_line_4" model="op.fees.terms.line">
            <field name="due_days">30</field>
            <field name="value">50.0</field>
        </record>

        <record id="op_fees_term_line_5" model="op.fees.terms.line">
            <field name="due_days">30</field>
            <field name="value">50.0</field>
        </record>

        <record id="op_fees_term_line_6" model="op.fees.terms.line">
            <field name="due_days">30</field>
            <field name="value">100.0</field>
        </record>

        <record id="op_fees_term_line_7" model="op.fees.terms.line">
            <field name="due_days">15</field>
