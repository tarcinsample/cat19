        </record>

        <record id="op_fees_term_date_line_9" model="op.fees.terms.line">
            <field name="due_date" eval='(DateTime.today() + relativedelta(days=30))'/>
            <field name="value">30.0</field>
        </record>

        <record id="op_fees_term_date_line_10" model="op.fees.terms.line">
            <field name="due_date" eval='(DateTime.today() + relativedelta(days=60))'/>
            <field name="value">40.0</field>
        </record>

</odoo>
